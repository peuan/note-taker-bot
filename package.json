{
  "name": "@tarikhagustia/notulen",
  "version": "1.4.3",
  "description": "A simple Google meet note taking app with AI transcription",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build-ts": "tsc",
    "build": "npm run clean && npm run build-ts",
    "example": "node --inspect -r ts-node/register examples/example.ts",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w",
    "dev": "npm run watch",
    "build:production": "npm install --production=false && npm run build && npm ci",
    "start": "NODE_ENV=production node dist/server.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.15.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "ffmpeg-static": "^5.2.0",
    "morgan": "^1.10.0",
    "pino": "^9.3.2",
    "pino-pretty": "^11.2.2",
    "puppeteer": "^22.13.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "puppeteer-stream": "^3.0.15",
    "typescript": "^5.5.3"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "ts-node": "^10.9.2"
  }
}
